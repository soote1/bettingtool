{
    "wait_time":5,
    "workers":[
        {
            "module":"processor.sample.arbitragebetting.workers",
            "class":"OddsConsumer",
            "args":[
                {
                    "host_name":"localhost",
                    "queue_name":"caliente_odds_queue",
                    "durable":true,
                    "prefetch_count":1,
                    "decode_format":"utf-8"
                },
                {
                    "module":"pythontools.actionmanager.manager",
                    "class":"ActionManager",
                    "args":[
                        {
                            "max_attempts":2,
                            "initial_action":{
                                "module":"processor.sample.arbitragebetting.actions",
                                "class":"DeserializeJsonAction",
                                "config":{"next_action":"incomplete_scores_filter"}
                            },
                            "incomplete_scores_filter":{
                                "module":"processor.sample.arbitragebetting.actions",
                                "class":"IncompleteScoresFilter",
                                "config":{
                                    "target_scores":["0-0","1-0","0-1","1-1","2-0","0-2","2-1","1-2","2-2","3-0","0-3","3-1","1-3","3-2","2-3","3-3"],
                                    "next_action":"search_optimal_solutions_action"
                                }
                            },
                            "search_optimal_solutions_action":{
                                "module":"processor.sample.arbitragebetting.actions",
                                "class":"SearchOptimalWagerSetAction",
                                "config":{
                                    "max_budget":10000,
                                    "gene_domain":[200, 600],
                                    "population_size":20,
                                    "mutation_rate":0.01,
                                    "gene_count":16,
                                    "max_iterations":100,
                                    "next_action":"enqueue_message_action"
                                }
                            },
                            "enqueue_message_action":{
                                "module":"processor.sample.arbitragebetting.actions",
                                "class":"EnqueueMessageAction",
                                "config":{
                                    "producer_config": {
                                        "host_name":"localhost",
                                        "queue_name":"arbitrage_outcomes_queue",
                                        "durable":true,
                                        "delivery_mode":2,
                                        "exchange":""
                                    },
                                    "next_action":""
                                }
                            }
                        }
                    ]
                }
            ]
        }
    ]
}